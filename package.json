{
  "private": true,
  "name": "admin",
  "version": "1.0.0",
  "main": "electron/main.js",
  "description": "这是一个测试的exe文件",
  "author": "lzh",
  "scripts": {
    "start": "vite | electron .",
    "exe": "vite build && electron-builder",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "build": {
    "appId": "com.example.app",
    "productName": "考勤",
    "copyright": "Copyright © 2023 lsl",
    "directories": {
      "output": "dist_electron"
    },
    "win": {
      "icon": "./build/electron-icon/icon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "files": [
      "./dist",
      "./electron",
      "!**/node_modules/**/{*.md,*.tsbuildinfo,.gitignore}",
      "node_modules/axios"
    ],
    "asar": false,
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "./build/electron-icon/icon.ico",
      "uninstallerIcon": "./build/electron-icon/icon.ico",
      "installerHeaderIcon": "./build/electron-icon/icon.ico",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    },
    "electronDownload": {
      "mirror": "https://npmmirror.com/mirrors/electron/"
    }
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.3",
    "app-builder-lib": "^25.0.0-alpha.8",
    "autoprefixer": "^10.4.7",
    "electron": "^30.0.7",
    "electron-builder": "^24.13.3",
    "electron-builder-squirrel-windows": "^25.0.0-alpha.8",
    "electron-devtools-installer": "^3.2.0",
    "electron-icon-builder": "^2.0.1",
    "unplugin-auto-import": "^0.8.8",
    "unplugin-vue-components": "^0.19.6",
    "vant": "^4.8.10",
    "vite": "2.9.12",
    "vite-plugin-ejs": "1.4.4",
    "vite-plugin-minify": "^1.5.2",
    "vite-plugin-mp": "^2.0.0"
  },
  "dependencies": {
    "@vant/touch-emulator": "^1.4.0",
    "axios": "^0.27.2",
    "combinate": "^1.1.11",
    "dayjs": "^1.11.3",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "qs": "^6.10.5",
    "sass": "^1.53.0",
    "scss": "^0.2.4",
    "view-ui-plus": "^1.3.0",
    "vue": "3.2.37"
  }
}
